@using Newtonsoft.Json;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "_LayoutLoginSignUp";
}

@section Head{
    <link rel="stylesheet" href="~/css/SignUp/SignUp.css"/>
    <link rel="stylesheet" href="~/lib/sweetalert2/sweetalert2.min.css"/>
}


@await Html.PartialAsync("~/Pages/Shared/_SignUp.cshtml");

@section Scripts{
    <script src="~/lib/sweetalert2/sweetalert2.js"></script>

    @if (TempData["accountExistMessage"] != null)
    {
        <script>
            let accountExistMessage = @Html.Raw(JsonConvert.SerializeObject(TempData["accountExistMessage"]));
            // console.log(accountExistMessage);
            Swal.fire({
                icon: 'error',
                title: 'Oops...',
                text: accountExistMessage,
            });
        </script>
    }
}
