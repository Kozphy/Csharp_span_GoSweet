@model Officel.Models.HomepageModels
<div class="container">
    <!-- 中心區塊劃分 -->
    <div class="row gx-3 d-flex justify-content-center">
        <!-- 廠商資訊 -->
        <div class="salerinfo col-11 col-xl-3 m-1 d-flex justify-content-center align-items-center">
            <figure class="figure m-1">
                <!-- ★★★★★需要廠商圖片★★★★★ -->
                <img src="~/img/shanni.jpg" class="figure-img img-fluid rounded" alt="ManufacturersLogo" id="ManufacturersLogo">
                <figcaption class="figure-caption"><span class="h4">歡迎回來!</span></figcaption>
            </figure>
        </div>
        <div class="salerinfo col-11 col-xl-8 m-1 p-3 d-flex flex-column justify-content-around">
            <!-- ★★★★★需要廠商評分★★★★★ -->
                    <div class="h3 fw-bold">商場評價: 
                        <span class="h4">
                            <div class="star star-lock">
                                <input type="radio" name="rdStarLock" value="1" placeholder="星級評測"/>
                                <input type="radio" name="rdStarLock" value="2" placeholder="星級評測"/>
                                <input type="radio" name="rdStarLock" value="3" placeholder="星級評測"/>
                                <input type="radio" name="rdStarLock" value="4" placeholder="星級評測" checked="checked"/>
                                <input type="radio" name="rdStarLock" value="5" placeholder="星級評測"/>
                            </div>
                  </span>
                 (<span class="h4" id="NumberOrder">
                999
               </span>)
        </div>
            <div class="h3 fw-bold">當月表現<br /></div>
            <div class="mb-3 h4">
                <div class="row d-flex justify-content-around">
                    <!-- ★★★★★需要廠商訂單數★★★★★ -->
                    <div class="col-12 col-xl-3">總訂單數: <span class="performance-number">@Model.ThisMonthOrdersTotal</span></div>
                    <!-- ★★★★★需要廠商訂單數★★★★★ -->
                    <div class="col-12 col-xl-3">總寄件數: <span class="performance-number">@Model.ThisMonthShippedTotal</span></div>
                    <!-- ★★★★★需要廠商訂單數★★★★★ -->
                    <div class="col-12 col-xl-5">總銷售額: <span class="performance-number">@Model.ThisMonthReveune</span></div>

                </div>
            </div>
            <div class="h3 fw-bold">前月表現<br /></div>
            <div class="h4">
                <div class="row d-flex justify-content-around align-items-center">
                    <!-- ★★★★★需要廠商訂單數★★★★★ -->
                    <div class="col-12 col-xl-3">總訂單數: <span class="performance-number">@Model.LastMonthOrdersTotal</span></div>
                    <!-- ★★★★★需要廠商訂單數★★★★★ -->
                    <div class="col-12 col-xl-3">總寄件數: <span class="performance-number">@Model.LastMonthShippedTotal</span></div>
                    <!-- ★★★★★需要廠商訂單數★★★★★ -->
                    <div class="col-12 col-xl-5">總銷售額: <span class="performance-number">@Model.LastMonthReveune</span></div>
                </div>
            </div>
            <a class="col-12 pt-3 h6 text-end link-underline-light text-secondary">
                更新日期：<span class="update" onclick="refresh()">
                    <!-- ★★★★★需要廠商訂單數★★★★★ -->
                    2023-06-01 ~ 2023-06-28
                </span>
            </a>
        </div>
    </div>
