    <div class="row d-flex justify-content-center" id="RecentlyOrder">
        <div class="col-11 m-1 salerinfo">
            <div class="m-1 h2">最近訂單</div>
            <hr>
            <form>
                <table class="Homepagetable table table-hover">
                    <thead>
                        <tr>
                            <!-- ★★★★★需要廠商訂單數★★★★★ -->
                            <th class="HomepageTableNumber" scope="col">#</th>
                            <th class="HomepageTalbeData" scope="col"><span class="h4">訂單成立日期</span></th>
                            <th class="HomepageTalbeData" scope="col"><span class="h4">訂單編號</span></th>
                            <th class="HomepageTalbeData" scope="col"><span class="h4">品項</span></th>
                        </tr>
                    </thead>
                    <tbody class="h5">
                        @{
                            int orderNumber = 1;
                            foreach(var WL in Model.WaitingList)
                            {
                                <tr>
                                    <th class="HomepageTableNumber" scope="row">@orderNumber .</th>
                                    <td class="HomepageTalbeData"><span class="h5">@WL.OrderDate</span></td>
                                    <td class="HomepageTalbeData"><a class="h5" href="./">@WL.OrderID</a></td>
                                    <td class="HomepageTalbeData"><span class="h5">@WL.Products</span></td>
                                </tr>
                            orderNumber++;
                            }
                        }
                    </tbody>
                </table>
            </form>
        </div>
    </div>
    <!-- 最近消息 -->
    <div class="row gx-3 d-flex justify-content-center" id="RecentlyStatus">
        <div class="salerinfo col-6 m-1">
            <div class="h2">最近評論</div>
            <!-- ★★★★★需要廠商訂單數★★★★★ -->
            <table class="Homepagetable table table-hover">
                <thead>
                         <tr>
                            <!-- ★★★★★需要廠商訂單數★★★★★ -->
                            <th class="HomepageTableNumber" scope="col">#</th>
                            <th class="HomepageTalbeData" scope="col"><span class="h4">日期</th>
                            <th class="HomepageTalbeData" scope="col"><span class="h4">內容</span></th>
                        </tr>
                </thead>
                <tbody class="h5">
                    @{
                        int commentNumber = 1;
                        foreach (var RC in Model.RecentlyComments)
                        {
                            <tr>
                                <td class="HomepageTableNumber" th scope="row">@commentNumber .</td>
                                <td class="HomepageTalbeData">@RC.CommentDate</td>
                                <td class="HomepageTalbeData">@RC.Content</td>
                            </tr>
                            commentNumber++;
                        }
                    }
                </tbody>
            </table>
        </div>
        <div class="salerinfo col-5 m-1">
            <div class="h2">最近熱賣</div>
            <!-- ★★★★★需要廠商訂單數★★★★★ -->
                        <table class="Homepagetable table table-hover">
                <thead>
                         <tr>
                            <!-- ★★★★★需要廠商訂單數★★★★★ -->
                            <th class="HomepageTableNumber" scope="col">#</th>
                            <th class="HomepageTalbeData" scope="col"><span class="h4">商品名</th>
                            <th class="HomepageTalbeData" scope="col"><span class="h4">數量</span></th>
                        </tr>
                </thead>
                <tbody class="h5">
                    @{
                        int hotNumber = 1;
                        foreach (var HS in Model.HotSale)
                        {
                                    <tr>
                                        <td class="HomepageTableNumber" scope="row">@hotNumber .</td>
                                        <td class="HomepageTalbeData">@HS.ProductName</td>
                                        <td class="HomepageTalbeData">@HS.Quentity</td>
                                    </tr>
                            hotNumber++;
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

